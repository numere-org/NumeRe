PARSERFUNCS_LISTCMD_TABLEHEAD=							Befehl:      Syntax:        Funktion:
PARSERFUNCS_LISTCMD_CMD_ABORT_[FLOWCTRL]=				abort                       - Abbrechen einer Schleifendeklaration oder einer Plotkomposition
PARSERFUNCS_LISTCMD_CMD_ABOUT_[MAINCTRL]=				about                       - rechtliche Informationen zu NumeRe
PARSERFUNCS_LISTCMD_CMD_APPEND_[lOADSAVE]=				append       NM [-PAR]      - Dateien an Daten im Speicher anhängen
PARSERFUNCS_LISTCMD_CMD_AUDIO_[LOADSAVE]=				audio        OB [-PAR]      - Erzeugt eine WAVE-Datei aus einem Datensatz
PARSERFUNCS_LISTCMD_CMD_BREAK_[FLOWCTRL]=				break                       - Schleifendurchlauf abbrechen
PARSERFUNCS_LISTCMD_CMD_CACHE_[DATACTRL]=				cache        -PAR           - Optionen auf dem Cache
PARSERFUNCS_LISTCMD_CMD_CLEAR_[DATACTRL]=				clear        OB [-PAR]      - Entfernt alle Caches/alle Elemente im string()-Objekt unwiderbringlich von diesem Rechner
PARSERFUNCS_LISTCMD_CMD_COMPOSE_[PLOTTING]=				compose                     - Startet die Komposition mehrerer Plotstile
PARSERFUNCS_LISTCMD_CMD_CONT_[PLOTTING]=				cont         EX [-set PAR]  - Erzeugung eines Konturlinien-Plots
PARSERFUNCS_LISTCMD_CMD_CONT3D_[PLOTTING]=				cont3d       EX [-set PAR]  - Erzeugung eines 3D-Konturlinien-Plots
PARSERFUNCS_LISTCMD_CMD_CONTINUE_[FLOWCTRL]=			continue                    - Restlichen Schleifendurchlauf überspringen
PARSERFUNCS_LISTCMD_CMD_COPY_[MAINCTRL]=				copy         OB [-PAR]      - Kopiert (je nach Ausdruck) Einträge/Spalten/Untertabellen zwischen Datenobjekten oder Dateien.
PARSERFUNCS_LISTCMD_CMD_DATA_[DATACTRL]=				data         -PAR           - Optionen auf Datensätzen
PARSERFUNCS_LISTCMD_CMD_DATAGRID_[DATACREATION]=		datagrid     EX -PAR        - Datengitter erzeugen
PARSERFUNCS_LISTCMD_CMD_DEFINE_[DEF]=					define       EX [-set PAR]  - Ermöglicht die Definition eigener Funktionen.
PARSERFUNCS_LISTCMD_CMD_DELETE_[DATACTRL_MAINCTRL]=		delete       OB [-PAR]      - Entfernt Einträge aus dem Cache/dem string()-Objekt
PARSERFUNCS_LISTCMD_CMD_DENS_[PLOTTING]=				dens         EX [-set PAR]  - Erzeugung eines Dichte-Plots
PARSERFUNCS_LISTCMD_CMD_DENS3D_[PLOTTING]=				dens3d       EX [-set PAR]  - Erzeugung eines 3D-Dichte-Plots
PARSERFUNCS_LISTCMD_CMD_DIFF_[EVAL]=					diff         EX -set PAR    - Ruft die Funktion zur numerischen Differentiation einer Funktion oder eines Datensatzes auf.
PARSERFUNCS_LISTCMD_CMD_DRAW_[PLOTTING]=				draw         EX [-set PAR]  - Zeichenmodus.
PARSERFUNCS_LISTCMD_CMD_DRAW3D_[PLOTTING]=				draw3d       EX [-set PAR]  - 3D-Zeichenmodus.
PARSERFUNCS_LISTCMD_CMD_EDIT_[MAINCTRL]=				edit         OB             - Öffnet einen Cache, eine Datei oder ein Verzeichnis zum Bearbeiten
PARSERFUNCS_LISTCMD_CMD_ELSE_[FLOWCTRL]=				else                        - Startet die Definition, die beim gegenteiligen Fall von "if" bzw. "elseif", ausgeführt werden soll.
PARSERFUNCS_LISTCMD_CMD_ELSEIF_[FLOWCTRL]=				elseif       (EX)           - Startet eine bedingte Definition, die beim gegenteiligen Fall von "if" bzw. "elseif", ausgeführt werden soll.
PARSERFUNCS_LISTCMD_CMD_ENDCOMPOSE_[PLOTTING]=			endcompose                  - Beendet die Definition einer zuvor begonnenen Plotkomposition
PARSERFUNCS_LISTCMD_CMD_ENDFOR_[FLOWCTRL]=				endfor       [-PAR]         - Beendet die Definition einer zuvor begonnenen Zählschleife
PARSERFUNCS_LISTCMD_CMD_ENDIF_[FLOWCTRL]=				endif                       - Beendet die Definition einer IF-Verzweigung
PARSERFUNCS_LISTCMD_CMD_ENDPROCEDURE_[FLOWCTRL_PROC]=	endprocedure                - Beendet die Definition einer zuvor begonnenen NumeRe-Prozedur
PARSERFUNCS_LISTCMD_CMD_ENDWHILE_[FLOWCTRL]=			endwhile     [-PAR]         - Beendet die Definition einer zuvor begonnenen While-Schleife
PARSERFUNCS_LISTCMD_CMD_EVAL_[DATACREATION]=			eval         EX -set PAR    - Berechnet Funktionswerte des Ausdrucks
PARSERFUNCS_LISTCMD_CMD_EXPLICIT_[MAINCTRL]=			explicit     CMD            - Deaktiviert alle Plugins für das direkt darauffolgende Kommando
PARSERFUNCS_LISTCMD_CMD_EXPORT_[LOADSAVE]=				export       OB [-PAR]      - Textexport eines Datensatzes
PARSERFUNCS_LISTCMD_CMD_EXTREMA_[EVAL]=					extrema      EX -set PAR    - Ruft die Funktion zur numerischen Bestimmung der Extremstellen eines Ausdrucks oder eines Datensatzes auf.
PARSERFUNCS_LISTCMD_CMD_FFT_[EVAL]=						fft          OB [-PAR]      - Fourier-Transformation eines Datensatzes
PARSERFUNCS_LISTCMD_CMD_FIND_[MAINCTRL]=				find         NM             - NumeRe-Stichwortsuche
PARSERFUNCS_LISTCMD_CMD_FIT_[EVAL]=						fit          OB -PAR        - Funktionen über Parameter an Datensätze anpassen
PARSERFUNCS_LISTCMD_CMD_FITW_[EVAL]=					fitw         OB -PAR        - Funktionen über Parameter an Datensätze mit Fehlerwerten anpassen
PARSERFUNCS_LISTCMD_CMD_FOR_[FLOWCTRL]=					for          (EX)           - Startet die Definition einer Zählschleife.
PARSERFUNCS_LISTCMD_CMD_GET_[MAINCTRL]=					get          -PAR           - Programmeinstellungen abfragen
PARSERFUNCS_LISTCMD_CMD_GLOBAL_[MISC]=					global       EX/CMD         - Eine Scriptzeile als global deklarieren
PARSERFUNCS_LISTCMD_CMD_GRAD_[PLOTTING]=				grad         EX [-set PAR]  - Erzeugung eines Gradienten-Plots
PARSERFUNCS_LISTCMD_CMD_GRAD3D_[PLOTTING]=				grad3d       EX [-set PAR]  - Erzeugung eines 3D-Gradienten-Plots
PARSERFUNCS_LISTCMD_CMD_HELP_[MAINCTRL]=				help         [NM]           - NumeRe-Hilfe
PARSERFUNCS_LISTCMD_CMD_HIST_[EVAL]=					hist         OB [-PAR]      - Erzeugen eines Histogrammes
